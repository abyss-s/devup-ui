{
  "name": "@devup-ui/eslint-plugin",
  "version": "0.1.0",
  "description": "eslint plugin for devup-ui",
  "main": "dist/index.cjs",
  "type": "module",
  "scripts": {
    "test": "vitest run --coverage",
    "test:s": "vitest run -u",
    "lint": "eslint",

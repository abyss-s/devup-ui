{
  "name": "@devup-ui/react",
  "description": "Zero Config, Zero FOUC, Zero Runtime, CSS in JS Preprocessor",
  "repository": "https://github.com/dev-five-git/devup-ui",
  "author": "devfive",
  "license": "Apache-2.0",
  "homepage": "https://devup-ui.com",
  "bugs": {
